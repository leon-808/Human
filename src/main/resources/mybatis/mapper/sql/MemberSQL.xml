<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="project.example.demo.MemberDAO">
	<insert id="submit_signup">
		insert into member
		values (#{param1},#{param2},#{param3},
		#{param4},#{param5},#{param6})
	</insert>
	
	<select id="check_duplicateID" resultType="_int">
		select count(id)
		from member
		where id = #{param1}
	</select>
	
	<select id="get_id" resultType="string">
		select id
		from member
		where id = #{param1} and pw =#{param2}
	</select>
	
	<select id="check_phone" resultType="_int">
		select count(phone)
		from member
		where phone = #{param1}
	</select>

</mapper>